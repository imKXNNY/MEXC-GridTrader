# ============================================================================= #
# Example .env file for TradeSage                                               #
# ============================================================================= #
# Rename this file to .env and replace the placeholder values with your actual  #
# API key and secret key for your preferred exchange.                           #
# ============================================================================= #

# Exchange Credentials
EXCHANGE_API_KEY=ENTER_YOUR_API_KEY_HERE
EXCHANGE_SECRET_KEY=ENTER_YOUR_SECRET_KEY_HERE

# Exchange Selection (supported: mexc, binance, kucoin, etc.)
EXCHANGE_NAME=mexc

# Debugging Variables
FLASK_DEBUG=True
LOG_LEVEL=DEBUG
